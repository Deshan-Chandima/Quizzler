<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.quizzlerapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizzlerApp"
        tools:targetApi="33">

        <activity
            android:name=".DownloadQuizActivity"
            android:exported="false" />
        <activity
            android:name=".EditQuizActivity"
            android:exported="false" />
        <activity
            android:name=".QuizMenu2Activity"
            android:exported="false" />
        <activity
            android:name=".ViewScoreActivity"
            android:exported="false" />
        <activity
            android:name=".QuizGameActivity"
            android:exported="false" />
        <activity
            android:name=".QuizMenuActivity"
            android:exported="false" />
        <activity
            android:name=".CreateQuizActivity"
            android:exported="false" />
        <activity
            android:name=".WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LastQuizScoreActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>